    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "react-is": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "recharts": "^3.2.1",
    "sqlite3": "^5.1.7"
  },
  "build": {
    "appId": "com.tcc.school",
